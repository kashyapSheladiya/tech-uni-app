<div class="row">
    <div class="col s12">
      <h3 class= "center-align"><b>Students List</b></h3>
    </div>
</div>

<div class="row">
  <% @students.all.each do |student| %>
    <div class="col s6 m6">
      <div class="card orange darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= student.name%></span>
          <span><%= student.email %></span>
          <br>
          <span>Course Enrollment: none</span>
        </div>
        <div class="card-action blue">
          <a href="#">Profile</a>
        </div>
      </div>
    </div>
  <% end %>
</div>